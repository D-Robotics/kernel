*/
};

&i2c0 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	tmp75c@48 {
		compatible = "ti,tmp75c";
		reg = <0x48>;
        };
};
&i2c1 {
	status = "okay";
};

&i2c4 {
	status = "okay";
};
&i2c5 {
	status = "okay";
};

&pinctrl {
	status = "okay";
	gpioirq-bank-cfg = <85 0 0 0>;
};

&uart0 {
	status = "okay";
};

&ips {
	status = "okay";
};

&isp {
	status = "okay";
};

&ipu {
	status = "okay";
};

&sif {
	status = "okay";
};

&mipi_host0 {
	status = "okay";
};

&mipi_host1 {
	status = "okay";
};

&mipi_host2 {
	status = "okay";
};

&mipi_host3 {
	status = "okay";
};

&mipi_dev0 {
	status = "okay";
};

&mipi_dphy {
	status = "okay";
};

&dma {
	status = "okay";
};

&x2dmatest {
	status = "disable";
};

&rtc {
	status = "okay";
};

&pwm_0 {
	status = "disabled";
};

&spi0 {
	status = "okay";
};

&ethernet {
	status = "okay";

	/* local-mac-address = [ 00 11 22 33 44 55 ]; */
	phy-handle = <&phy1>;
	phy-mode = "rgmii-id";
	mdio {
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		phy1: phy@e {
			compatible = "marvell,88E1111","ethernet-phy-ieee802.3-c22";
			reg = <0xe>;
		};
	};
};

&cnn0 {
	status = "okay";
};

&cnn1 {
	status = "okay";
};

&iar {
        status = "okay";
};

&bifspi {
	status = "okay";
};

&bifsd {
	status = "okay";
	/*cd-gpio = <77>;*/
};

&bifbase {
	bifbase_irq_pin = <85>;
	bifbase_tri_pin = <84>;
};

&nor_flash {
	status = "disabled";
	#address-cells = <1>;
	#size-cells = <0>;
	qspi-mode = <4>;

	flash@0 {
		compatible = "gd256lq256d";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0x0>;
		spi-tx-bus-width = <4>;
		spi-rx-bus-width = <4>;
		spi-max-frequency = <20000000>; /* For FPGA max clk is 20Mhz */

		partition@qspi-uboot {
			label = "qspi-uboot";
			reg = <0x200000 0x200000>;
		};
		partition@qspi-linux {
			label = "qspi-linux";
			reg = <0x400000 0x1000000>;
		};
		partition@qspi-device-tree {
			label = "qspi-device-tree";
			reg = <0x1400000 0x100000>;
		};
		partition@qspi-rootfs {
			label = "qspi-rootfs";
			reg = <0x1500000 0x500000>;
		};
	};
};

&nand_flash {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	qspi-mode = <4>;

	spi-nand@0 {
		compatible = "spi-nand";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0x0>;
		spi-tx-bus-width = <4>;
		spi-rx-bus-width = <4>;
		spi-max-frequency = <10000000>; /* For FPGA max clk is 20Mhz */

		bootloader@0 {
			label = "bootloader";
			reg = <0 0x300000>;
		};
		sys@300000 {
			label = "sys";
			reg = <0x300000 0xA00000>;
		};
		rootfs@D00000 {
			label = "rootfs";
			reg = <0xD00000 0>;
		};
	};
};

&sdio0 {
	status = "okay";
	bus-width = <4>;
	cap-mmc-highspeed;
	mmc-hs200-1_8v;
	non-removable;
	disable-wp;
};

&sdio1 {
	status = "okay";
	bus-width = <4>;
	no-mmc;
	no-sdio;
	cap-sd-highspeed;
	sd-uhs-sdr12;
	sd-uhs-sdr25;
	sd-uhs-sdr50;
	sd-uhs-sdr104;
	uhs-180v-gpio = <120>;
	broken-cd;
	disable-wp;
};

&watchdog {
	status = "disabled";
};

&timer1 {
	status = "okay";
};

&i2s0 {
	status = "okay";
	clocks = <&i2s0_mclk>, <&i2s0_bclk>;
	clock-names = "i2s_mclk", "i2s_bclk";
};

&i2s1 {
	status = "okay";
	clocks = <&i2s1_mclk>, <&i2s1_bclk>;
	clock-names = "i2s_mclk", "i2s_bclk";
};

&jpu {
	status = "okay";
};

&vpu {
	status = "okay";
};
