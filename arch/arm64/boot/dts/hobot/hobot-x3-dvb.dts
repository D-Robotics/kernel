/dts-v1/;
#include "hobot-xj3.dtsi"
#include "hobot-pinctrl-xj3.dtsi"
#include "hobot-clk-xj3.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/phy/phy.h>

/ {
	model = "Hobot X3 SOC MP DVB";
	compatible = "hobot,x3-soc";

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;
		i2c0	= &i2c0;
		i2c1	= &i2c1;
		i2c2	= &i2c2;
		i2c3	= &i2c3;
		i2c4	= &i2c4;
		i2c5	= &i2c5;
		i2s0	= &i2s0;
		i2s1	= &i2s1;
		pwm0	= &pwm_0;
		pwm1	= &pwm_1;
		pwm2	= &pwm_2;
		bifspi	= &bifspi;
		bifsd   = &bifsd;
		mmc0	= &sdio0;
		dma	= &dma;
		x2dmatest = &x2dmatest;
		mipihost0 = &mipi_host0;
		mipihost1 = &mipi_host1;
		mipihost2 = &mipi_host2;
		mipihost3 = &mipi_host3;
		mipidev0 = &mipi_dev0;
	};

	chosen {
		bootargs = "earlycon loglevel=8 console=ttyS0 clk_ignore_unused";
		stdout-path = "serial0:921600n8";
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x0 0x0 0x40000000>;
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		/*Reserved 1M memory for bifbase*/
		bifbase_reserved: bifbase_reserved@0x02000000 {
			reg = <0x0 0x02000000 0x0 0x00100000>;
			no-map;
		};

		/* Reserved 4K memory for swinfo */
		sw_reserved: sw_reserved@0x020ff000 {
			reg = <0x0 0x020ff000 0x0 0x1000>;
			no-map;
		};

		bifsd_reserved: bifsd_reserved@0x3D089000 {
			reg = <0x0 0x3D089000 0x0 0x1000>;
			no-map;
		};

		iar_reserved: iar_reserved@0x3D08A000 {
			reg = <0x0 0x3D08A000 0x0 0x2D46000>;
			no-map;
		};

		/* Reserved 192K memory for function call */
		fc_reserved: fc_reserved@0x3FDD0000 {
			reg = <0x0 0x3FDD0000 0x0 0x00030000>;
		};

		/*Reserved 2MB memory for BL31*/
		bl31_reserved: bl31_reserved@0x40000000 {
			reg = <0x0 0x3FE00000 0x0 0x200000>;
			no-map;
		};

		/* Reserved 256MB memory for cnn subsystem */
		ion_reserved: ion_reserved@0x1D089000 {
			compatible = "ion-pool";
			reg = <0x0 0x1D089000 0x0 0x10000000>;
		};

		/* Reserved 256MB memory for ipu subsystem */
		ipu_reserved: ipu_reserved@0x2D089000 {
			reg = <0x0 0x2D089000 0x0 0x10000000>;
			/*no-map;*/
		};

	};
/*
