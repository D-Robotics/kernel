#include <dt-bindings/interrupt-controller/arm-gic.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/thermal/thermal.h>

 / {
	compatible = "hobot,x3";
	#address-cells = <2>;
	#size-cells = <2>;

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu0: cpu@0 {
			compatible = "arm,cortex-a53", "arm,armv8";
			device_type = "cpu";
			enable-method = "psci";
			operating-points-v2 = <&cpu_opp_table>;
			clocks = <&cpu_mux_clk>;
			#cooling-cells = <2>;
			cooling-min-level = <0>;
			cooling-max-level = <2>;
			dynamic-power-coefficient = <120>;
			reg = <0x0>;
			cpu-idle-states = <&CPU_SLEEP_0>;
		};

		cpu1: cpu@1 {
			compatible = "arm,cortex-a53", "arm,armv8";
			device_type = "cpu";
			enable-method = "psci";
			cpu-release-addr = <0 0xA6000278>;
			operating-points-v2 = <&cpu_opp_table>;
			clocks = <&cpu_mux_clk>;
			reg = <0x1>;
			cpu-idle-states = <&CPU_SLEEP_0>;
		};

		cpu2: cpu@2 {
			compatible = "arm,cortex-a53", "arm,armv8";
			device_type = "cpu";
			enable-method = "psci";
			cpu-release-addr = <0 0xA6000278>;
			operating-points-v2 = <&cpu_opp_table>;
			clocks = <&cpu_mux_clk>;
			reg = <0x2>;
			cpu-idle-states = <&CPU_SLEEP_0>;
		};

		cpu3: cpu@3 {
			compatible = "arm,cortex-a53", "arm,armv8";
			device_type = "cpu";
			enable-method = "psci";
			cpu-release-addr = <0 0xA6000278>;
			operating-points-v2 = <&cpu_opp_table>;
			clocks = <&cpu_mux_clk>;
			reg = <0x3>;
			cpu-idle-states = <&CPU_SLEEP_0>;
		};

		/*TODO: maybe need to change */
		idle-states {
			/*entry-method = "arm,psci";*/
			entry-method = "arm,psci";

			CPU_SLEEP_0: cpu-sleep-0 {
				compatible = "arm,idle-state";
				arm,psci-suspend-param = <0x0010000>;
				local-timer-stop;
				entry-latency-us = <300>;
				exit-latency-us = <600>;
				min-residency-us = <100000>;
			};
		};
	};

	/*TODO: maybe need to change or delete */
	cpu_opp_table: cpu_opp_table {
		compatible = "operating-points-v2";
		opp-shared;
		opp00 {
			opp-hz = /bits/ 64 <1199999988>;
			opp-microvolt = <1000000>;
			clock-latency-ns = <500000>;
		};
		opp01 {
			opp-hz = /bits/ 64 <599999994>;
			opp-microvolt = <1000000>;
			clock-latency-ns = <500000>;
		};
	};

	psci {
		compatible = "arm,psci-1.0";
		method = "smc";
	};

	gic: interrupt-controller@90001000 {
		compatible = "arm,gic-400", "arm,cortex-a53-gic";
		#interrupt-cells = <3>;
		reg = <0x0 0x90001000 0x0 0x1000>,		/* GICD */
			  <0x0 0x90002000 0x0 0x2000>;		/* GICC */
		interrupt-controller;
	};

	arch_timer: arch-timer{
		compatible = "arm,armv8-timer";
		interrupts = <GIC_PPI 13 (GIC_CPU_MASK_SIMPLE(2)
					 | IRQ_TYPE_LEVEL_LOW)>,
					 <GIC_PPI 14 (GIC_CPU_MASK_SIMPLE(2)
					 | IRQ_TYPE_LEVEL_LOW)>,
					 <GIC_PPI 11 (GIC_CPU_MASK_SIMPLE(2)
					 | IRQ_TYPE_LEVEL_LOW)>,
					 <GIC_PPI 10 (GIC_CPU_MASK_SIMPLE(2)
					 | IRQ_TYPE_LEVEL_LOW)>;
		interrupt-parent = <&gic>;
		clock-frequency = <24000000>;
	};

	rst: reset-controller {
		compatible = "hobot,x2-reset";
		#reset-cells = <2>;
		reg = <0x0 0xA1000000 0x0 0x1000>;
		x2,modrst-offset = <0x400>;
	};

	sysctrl: system-controller@0xA1000000 {
		compatible = "hobot,sysctrl";
		reg = <0x0 0xA1000000 0x0 0x1000>;
		reboot-offset = <0x530>;
	};

	pmu {
		compatible = "arm,cortex-a53-pmu", "arm,armv8-pmuv3";
		interrupt-parent = <&gic>;
		interrupts = <0 4 4>, <0 5 4>;
		interrupt-affinity = <&cpu0>,<&cpu1>;
	};

	thermal_zones: thermal-zones {
		soc_thermal: soc-thermal {
			polling-delay-passive = <500>; /* milliseconds */
			polling-delay = <1000>; /* milliseconds */
			status = "disabled";

			thermal-sensors = <&temp>;

			trips {
				threshold: trip-point@0 {
					temperature = <60000>;
					hysteresis = <0>;
					type = "passive";
				};
				target: trip-point@1 {
					temperature = <75000>;
					hysteresis = <0>;
					type = "passive";
				};
				soc_crit: soc-crit {
					temperature = <110000>;
					hysteresis = <0>; /* millicelsius */
					type = "critical";
				};
			};
			cooling-maps {
				map0 {
					trip = <&target>;
					cooling-device = <&cpu0 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
					contribution = <2048>;
				};
				map1 {
					trip = <&target>;
					cooling-device = <&cnn0 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
					contribution = <1024>;
				};
				map2 {
					trip = <&target>;
					cooling-device = <&cnn1 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
					contribution = <1024>;
				};
			};
		};

	};

	soc {
		compatible = "simple-bus";
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		uart0: serial@0xA5000000 {
			compatible = "hobot,x2-uart";
			reg = <0 0xA5000000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 29 4>;
			status = "disabled";
		};

		uart1: serial@0xA5001000 {
			compatible = "hobot,x2-uart";
			reg = <0 0xA5001000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 30 4>;
			status = "disabled";
		};

		uart2: serial@0xA5002000 {
			compatible = "hobot,x2-uart";
			reg = <0 0xA5002000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 31 4>;
			status = "disabled";
		};

		uart3: serial@0xA5003000 {
			compatible = "hobot,x2-uart";
			reg = <0 0xA5003000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 32 4>;
			status = "disabled";
		};


		usb: usb@0xB2000000 {
			compatible = "hobot,usb";
			reg = <0 0xB2000000 0 0x10000>,
			  <0 0xA1000000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 26 4>;
			/* dr_mode: {gadget, host, otg} */
			dr_mode = "host";
			/* phy_type: {utmi, utmi_wide, ulpi, serial, hsic} */
			phy_type = "utmi_wide";
			/*snps,dis_u2_susphy_quirk; */
			status = "okay";
		};

		i2s0: i2s@0xA5007000 {
			compatible = "hobot,x2-i2s";
			reg = <0 0xA5007000 0 0x100>,
				<0 0xA5007500 0 0x100>,
				<0 0xA1000000 0 0x500>;
			interrupt-parent = <&gic>;
			interrupts = <0 36 4>;
			resets = <&rst 0x50 18>;
			reset-names = "i2s";
			pinctrl-names = "default";
			pinctrl-0 = <&i2s0_func>;
			div_ws = <3855>;/*only for i2s mode*/
			ms = <1>;
			bclk_set = <2048000>;/* only for master mode,tianpai 48K is 6.144M*/
			mclk_set = <12288000>;/* only for master mode,default is 12.288M*/
			blck = <6144000>;/* only for master mode*/
			slot_width= <128>;/*128,64*/
			#status = "disabled";
		};

		i2s1: i2s@0xA5008000 {
			compatible = "hobot,x2-i2s";
			reg = <0 0xA5008000 0 0x100>,
				<0 0xA5008500 0 0x100>,
				<0 0xA1000000 0 0x500>;
			interrupt-parent = <&gic>;
			interrupts = <0 37 4>;
			resets = <&rst 0x50 19>;
			reset-names = "i2s";
			pinctrl-names = "default";
			pinctrl-0 = <&i2s1_func>;
			div_ws = <3855>;
			ms = <1>;
			bclk_set = <6144000>;/* only for master mode,tianpai is 6.144M*/
			mclk_set = <12288000>;/* only for master mode,default is 12.288M*/
			slot_width= <128>;/*128,64*/
			#status = "disabled";
		};

		i2sidma0: idma@0 {
			compatible = "x2-i2s0-idma";
			reg = <0 0xA5007000 0 0x100>,
				<0 0xA5007500 0 0x100>,
				<0 0xA1000000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 30 4>;
		};

		snd0: sndcard@0{
			compatible = "hobot, x2-snd0";
			i2s_mode = <4>;/*1:i2s mode; 4:dsp mode*/
			master = <1>;/*1:master; 4:slave*/
			channel_max = <8>;
		};

		i2c0: i2c@0xA5009000 {
			compatible = "hobot,x2-i2c";
			reg = <0 0xA5009000 0 0x100>;
			interrupt-parent = <&gic>;
			interrupts = <0 38 4>;
			clocks = <&i2c0_mclk>;
			clock-names = "i2c_mclk";
			resets = <&rst 0x50 11>;
			reset-names = "i2c0";
			#status = "disabled";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c0_func>;
		};

		i2c1: i2c@0xA500A000 {
			compatible = "hobot,x2-i2c";
			reg = <0 0xA500A000 0 0x100>;
			interrupt-parent = <&gic>;
			interrupts = <0 39 4>;
			clocks = <&i2c1_mclk>;
			clock-names = "i2c_mclk";
			resets = <&rst 0x50 12>;
			reset-names = "i2c1";
			#status = "disabled";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c1_func>;
		};

		i2c2: i2c@0xA500B000 {
			compatible = "hobot,x2-i2c";
			reg = <0 0xA500B000 0 0x100>;
			interrupt-parent = <&gic>;
			interrupts = <0 40 4>;
			clocks = <&i2c2_mclk>;
			clock-names = "i2c_mclk";
			resets = <&rst 0x50 13>;
			reset-names = "i2c2";
			#status = "disabled";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c2_func>;
		};

		i2c3: i2c@0xA500C000 {
			compatible = "hobot,x2-i2c";
			reg = <0 0xA500C000 0 0x100>;
			interrupt-parent = <&gic>;
			interrupts = <0 41 4>;
			clocks = <&i2c3_mclk>;
			clock-names = "i2c_mclk";
			resets = <&rst 0x50 14>;
			reset-names = "i2c3";
			#status = "disabled";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c3_func>;
		};

		i2c4: i2c@0xA5016000 {
			compatible = "hobot,x2-i2c";
			reg = <0 0xA5016000 0 0x100>;
			interrupt-parent = <&gic>;
			interrupts = <0 42 4>;
			clocks = <&i2c4_mclk>;
			clock-names = "i2c_mclk";
			resets = <&rst 0x50 15>;
			reset-names = "i2c4";
			#status = "disabled";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c4_func>;
		};

		i2c5: i2c@0xA5017000 {
			compatible = "hobot,x2-i2c";
			reg = <0 0xA5017000 0 0x100>;
			interrupt-parent = <&gic>;
			interrupts = <0 43 4>;
			clocks = <&i2c5_mclk>;
			clock-names = "i2c_mclk";
			resets = <&rst 0x50 16>;
			reset-names = "i2c5";
			#status = "disabled";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c5_func>;
		};

		usrdrv: usrdrv@0xA700C000 {
			compatible = "hobot,usrdrv";
			reg = <0 0xA500C000 0 0x100>;
			interrupt-parent = <&gic>;
			/* interrupt list : sif, ipu, pym, ldc, dwe*/
			interrupts = <0 75 4>, <0 73 4>;
		};

		ips: ips@0xA4000000 {
			compatible = "hobot,x2a-ips";
			reg = <0 0xA4000000 0 0x200>;
			interrupt-parent = <&gic>;
			interrupts = <0 85 4>;
			clocks = <&sif_mclk>, <&mipi_rx0_ipi_mclk>,
			<&mipi_rx1_ipi_mclk>, <&mipi_rx2_ipi_mclk>,
			<&mipi_rx3_ipi_mclk>, <&mipi_tx_ipi_mclk>,
			<&mipi_cfg_host_mclk>, <&pym_mclk>,
			<&mipi_dev_ref_clk>, <&mipi_host_ref_clk>;
			clock-names = "sif_mclk", "mipi_rx0_ipi",
			"mipi_rx1_ipi", "mipi_rx2_ipi", "mipi_rx3_ipi",
			"mipi_tx_ipi", "mipi_cfg_host", "pym_mclk",
			"mipi_dev_ref", "mipi_host_ref";
			status = "disabled";
		};

		ipu: ipu@0xA4040000 {
			compatible = "hobot,x2a-ipu";
			reg = <0 0xA4040000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 81 4>;
			status = "disabled";
		};

                pym: pym@0xA4042000 {
                        compatible = "hobot,x2a-pym";
                        reg = <0 0xA4042000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 83 4>;
                        status = "okay";
                };

                sif: sif@0xA4001000 {
                        compatible = "hobot,x2a-sif";
                        reg = <0 0xA4001000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 70 4>;
                        status = "disabled";
                };

		mipi_host: mipi_host@0xA4012000 {
			compatible = "hobot,x2-mipi-host";
			reg = <0 0xA4012000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 57 4>;
			status = "disabled";
		};

	        mipi_dev: mipi_dev@0xA4013000 {
			compatible = "hobot,x2-mipi-dev";
			reg = <0 0xA4013000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 49 4>;
			status = "disabled";
		};

		/* gdc0 */
		gdc0: gdc0@0xA4010000 {
			compatible = "hobot,gdc";
			reg = <0 0xA4010000 0 0x10000>;
			id = <0>;
			interrupt-parent = <&gic>;
			interrupts = <0 74 4>;
			status = "okay";
		};

		/* gdc1 */
		gdc1: gdc1@0xA4020000 {
			compatible = "hobot,gdc";
			reg = <0 0xA4020000 0 0x10000>;
			id = <1>;
			interrupt-parent = <&gic>;
			interrupts = <0 78 4>;
			status = "okay";
		};

                iar: iar@0xA4001000 {
                        compatible = "hobot,x2-iar";
                        reg = <0 0xA4301000 0 0x400>, <0 0xA4355000 0 0x1000>;
                        memory-region = <&iar_reserved>;
                        interrupt-parent = <&gic>;
                        interrupts = <0 69 4>;
                        resets = <&rst 0x40 6>;
                        reset-names = "iar";
                        status = "disabled";
                };

		/* isp0 */
		isp: isp@0xB3000000 {
			compatible = "hobot,x2a-isp";
			reg = <0x0 0xB3000000 0x0 0x00040000>;
			interrupt-parent = <&gic>;
			interrupts = <0 71 4>, <0 88 4>;
			interrupt-names = "ISP";
		};

		/* fake addr for sensor/lens/iq driver */
		sensor: soc_sensor@0x64300000 {
			compatible = "soc,sensor";
			reg = <0x0 0x64300000 0x0 0x001000>;
		};
		lens: soc_lens@0x64301000 {
			compatible = "soc,lens";
			reg = <0x0 0x64301000 0x0 0x001000>;
		};
		iq: soc_iq@0x64302000 {
			compatible = "soc,iq";
			reg = <0x0 0x64302000 0x0 0x001000>;
		};

		/* dwe */
		dwe: dwe@0xA4020000 {
		    compatible = "hobot,dwe";
		    //reg = <0 0xA4020000 0 0x10000>;
		    status = "okay";
		};

		ldc: ldc@0xA4030000 {
		    compatible = "hobot,x2a-ldc";
		    reg = <0 0xA4030000 0 0x1000>;
		    interrupt-parent = <&gic>;
		    interrupts = <0 75 4>;
		    status = "okay";
		};

		dis: dis@0xA4034000 {
		    compatible = "hobot,x2a-dis";
		    reg = <0 0xA4034000 0 0x1000>;
		    interrupt-parent = <&gic>;
		    interrupts = <0 73 4>;
		    status = "okay";
		};

		dis1: dis1@0xA4035000 {
		    compatible = "hobot,x2a-dis1";
		    reg = <0 0xA4035000 0 0x1000>;
		    //interrupt-parent = <&gic>;
		    //interrupts = <0 77 4>;
		    status = "okay";
		};

		jpu: jpu@0xA8010000 {
			compatible = "hobot,x2a_jpu";
			reg = <0 0xA8010000 0 0x10000>;
			interrupt-parent = <&gic>;
			interrupts = <0 67 4>;
			status = "disabled";
		};

		vpu: vpu@0xA8000000 {
			compatible = "hobot,x2a_vpu";
			reg = <0 0xA8000000 0 0x10000>;
			clocks = <&vpu_cclk>,<&vpu_bclk>;
			clock-names = "vpu_cclk","vpu_bclk";
			interrupt-parent = <&gic>;
			interrupts = <0 66 4>;
			status = "disabled";
		};

		dma: dma@0xA1005000 {
			compatible = "hobot,x2-dma";
			reg = <0 0xA1005000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 24 4>;
		};

		x2dmatest: x2dmatest {
			compatible = "hobot,x2-dma-test";
		};

		rtc: rtc@0xA6002000 {
			compatible = "hobot,x2-rtc";
			reg = <0 0xA6002000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 53 4>;
			status = "disabled";
		};

		pwm_0: pwm@0xA500D000 {
			compatible = "hobot,x2-pwm";
			reg = <0 0xA500D000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 44 4>;
			status = "disabled";
		};

		pwm_1: pwm@0xA500E000 {
			compatible = "hobot,x2-pwm";
			reg = <0 0xA500E000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 45 4>;
			status = "disabled";
		};

		pwm_2: pwm@0xA500F000 {
			compatible = "hobot,x2-pwm";
			reg = <0 0xA500F000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 46 4>;
			status = "disabled";
		};

		spi0: spi@0xA5004000 {
			compatible = "hobot,x2-spi";
			reg = <0 0xA5004000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 33 4>;
			resets = <&rst 0x50 5>;
			reset-names = "spi0";
			status = "disabled";
			pinctrl-names = "default";
			pinctrl-0 = <&spi0_func>;
			#address-cells = <1>;
			#size-cells = <0>;
			spidev@0x00 {
				compatible = "rohm,dh2228fv";
				spi-max-frequency = <20000000>;
				reg = <0>;
			};
		};

		spi1: spi@0xA5005000 {
			compatible = "hobot,x2-spi";
			reg = <0 0xA5005000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 34 4>;
			status = "disabled";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		spi2: spi@0xA5006000 {
			compatible = "hobot,x2-spi";
			reg = <0 0xA5006000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 35 4>;
			status = "disabled";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		nand_flash: nand@0xB0000000 {
			compatible = "hobot,x2-qspi-nand";
			status = "disabled";
			num-cs = <8>;
			reg = <0x0 0xB0000000 0x0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 25 4>;
			clocks = <&sys_qspi_aclk>;
			clock-names = "qspi_aclk";
		};

		nor_flash: nor@0xB0000000 {
			compatible = "hobot,x2-qspi-nor";
			status = "disabled";
			reg = <0x0 0xB0000000 0x0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 19 4>;
			clocks = <&sys_qspi_aclk>;
			clock-names = "qspi_aclk";
		};

		ethernet: ethernet@A5014000 {
			compatible = "snps,dwc-qos-ethernet-4.10a";
			reg = <0 0xA5014000 0 0x2000>;
			interrupt-parent = <&gic>;
			interrupts = <0 50 4>,<0 51 4>, <0 52 4>;
			interrupt-names = "rx-irq", "tx-irq", "mac-irq"; 

			interrupt_mode = <1>;
            use_riwt = <0>;

			snps,write-requests = <8>;
			snps,read-requests = <8>;
			snps,txpbl = <32>;
			snps,rxpbl = <16>;
			snps,clk_csr = <2>;

			snps,force_sf_dma_mode = <1>;

			pinctrl-names = "eth_state";
			pinctrl-0 = <&eth_func>;
			status = "disabled";
			snps,mtl-rx-config {
				snps,rx-queues-to-use = <4>;
				snps,rx-sched-sp;
				queue0 {
					snps,dcb-algorithm;
				};

				queue1 {

					snps,avb-algorithm;
					snps,route-ptp;
				};

				queue2 {
					snps,priority = <0x4>;
					snps,avb-algorithm;
				};

				queue3 {
					snps,avb-algorithm;
					snps,priority = <0x8>;
				};
			};
			snps,mtl-tx-config {
				snps,tx-queues-to-use = <4>;
				queue0 {
					snps,dcb-algorithm;
					snps,tx-sched-sp;
				};
				//for PTP (10%BW)
				queue1 {
					snps,avb-algorithm;
					snps,idle_slope = <0x38e>;
					snps,send_slope = <0x1ccc>;
					snps,high_credit = <0x556aaa>;
					snps,low_credit = <0xff57400>;
					snps,priority = <0x2>;
				};
				//class B traffic (25%BW)
				queue2 {
					snps,avb-algorithm;
					snps,idle_slope = <0x8e4>;
					snps,send_slope = <0x1800>;
					snps,high_credit = <0x7d0000>;
					snps,low_credit = <0xff736000>;
					snps,priority = <0x4>;
				};
				//classA (40%BW)
				queue3 {
					snps,avb-algorithm;
					snps,idle_slope = <0xe38>;
					snps,send_slope = <0x1333>;
					snps,high_credit = <0x4b0000>;
					snps,low_credit = <0xff8f8000>;
					snps,priority = <0x8>;
				};
			};
		};


		fb: fb {
			compatible = "hobot,x2-fb";
			status = "enabled";
		};

		timer0: timer@0xA1002000 {
			compatible = "hobot,x2-timer";
			status = "disabled";
			interrupts = <GIC_SPI 14 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>,
						 <GIC_SPI 15 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>,
						 <GIC_SPI 16 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>;
			interrupt-parent = <&gic>;
			reg = <0x0 0xA1002000 0x0 0x1000>;
			timer-freq = <20000000>;
		};

		timer1: timer@0xA1003000 {
			compatible = "hobot,x2-timer";
			status = "disabled";
			interrupts = <GIC_SPI 17 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>,
						 <GIC_SPI 18 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>,
						 <GIC_SPI 19 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>;
			interrupt-parent = <&gic>;
			reg = <0x0 0xA1003000 0x0 0x1000>;
			timer-freq = <20000000>;
		};

		timer2: timer@0xA1004000 {
			compatible = "hobot,x2-timer";
			status = "disabled";
			interrupts = <GIC_SPI 20 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>,
						 <GIC_SPI 21 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>,
						 <GIC_SPI 22 (GIC_CPU_MASK_SIMPLE(2)
						 | IRQ_TYPE_LEVEL_HIGH)>;
			interrupt-parent = <&gic>;
			reg = <0x0 0xA1004000 0x0 0x1000>;
			timer-freq = <20000000>;
		};

		watchdog: watchdog@0xA1002000 {
			compatible = "hobot,x2-wdt";
			reg = <0 0xA1002000 0 0x1000>;
			clocks = <&timer0_mclk>;
			clock-names = "watchdog_mclk";
			interrupt-parent = <&gic>;
			interrupts = <0 15 4>;
			timeout-sec = <10>;
			status = "disabled";
		};

		cnn0: cnn@0xA3000000 {
			compatible = "hobot,x2-cnn-host";
			reg = <0x0 0xA3000000 0x0 0x1000>,
				<0x0 0xA6000010 0x0 0x04>;
			memory-region = <&fc_reserved>;
			cnn-id = <0x0>;
			interrupt-parent = <&gic>;
			interrupts = <0 55 4>;
			resets = <&rst 0x20 0 >;
			reset-names = "cnn_rst";
			clocks = <&cnn0_aclk>,<&cnn0_mclk>;
			clock-names = "cnn_aclk","cnn_mclk";
			iso-bit = <1>;
			#cooling-cells = <2>;
			cooling-min-level = <0>;
			cooling-max-level = <4>;
			operating-points-v2 = <&cnn_opp_table>;
			busy_check = <1>;
			status = "disabled";
		};

		cnn1: cnn@0xA3001000 {
			compatible = "hobot,x2-cnn-host";
			reg = <0x0 0xA3001000 0x0 0x1000>,
				<0x0 0xA6000010 0x0 0x04>;
			memory-region = <&fc_reserved>;
			cnn-id = <0x1>;
			interrupt-parent = <&gic>;
			interrupts = <0 56 4>;
			resets = <&rst 0x20 1 >;
			reset-names = "cnn_rst";
			clocks = <&cnn1_aclk>,<&cnn1_mclk>;
			clock-names = "cnn_aclk","cnn_mclk";
			iso-bit = <5>;
			#cooling-cells = <2>;
			cooling-min-level = <0>;
			cooling-max-level = <4>;
			operating-points-v2 = <&cnn_opp_table>;
			busy_check = <1>;
			status = "disabled";
		};

		bifspi: bifspi@0xA1006000 {
			compatible = "hobot,x2-bifspi";
			reg = <0 0xA1006000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 16 4>;
			status = "disabled";
			resets = <&rst 0x00 3>;
			reset-names = "bifspi";
			pinctrl-names = "default";
			pinctrl-0 = <&bifspi_func>;
		};

		bifsd: bifsd@A1007000 {
			compatible = "hobot,bifsd";
			reg = <0x0 0xA1007000 0x0 0x1000>,
				  <0x0 0xA1000590 0x0 0x10>;
			interrupt-parent = <&gic>;
			interrupts = <0 23 4>;
			memory-region = <&bifsd_reserved>;
			status = "disabled";
			resets = <&rst 0x00 5>;
			reset-names = "bifsd";
			pinctrl-names = "default";
			pinctrl-0 = <&bifsd_func>;
		};

		sdio0: dwmmc@A5010000 {
			compatible = "hobot,x2-dw-mshc";
			reg = <0x0 0xA5010000 0x0 0x1000>;
			clock-freq-min-max = <100000 50000000>;
			clocks = <&sd0_cclk>;
			clock-names = "sdio_mclk";
			clock-frequency = <200000000>;
			fifo-depth = <0x100>;
			fifo-mode = <0>;
			bus-width = <4>;
			cap-mmc-highspeed;
			non-removable;
			no-sd;
			no-sdio;
			disable-wp;
			interrupt-parent = <&gic>;
			interrupts = <0 47 4>;
			status = "disabled";
			reset = <&rst 0x50 15>;
			reset-names = "reset";
			pinctrl-names = "default";
			pinctrl-0 = <&sdio0_func>;
		};

		sdio1: dwmmc@A5011000 {
			compatible = "hobot,x2-dw-mshc";
			reg = <0x0 0xA5011000 0x0 0x1000>;
			clock-freq-min-max = <100000 200000000>;
			clocks = <&sd1_cclk>;
			clock-names = "sdio_mclk";
			clock-frequency = <20000000>;
			fifo-depth = <0x100>;
			fifo-mode = <0>;
			interrupt-parent = <&gic>;
			interrupts = <0 48 4>;
			status = "disabled";
			reset = <&rst 0x50 16>;
			reset-names = "reset";
			pinctrl-names = "default";
			pinctrl-0 = <&sdio1_func>;
		};

		bifbase: bifbase {
			compatible = "hobot,bifbase";
			memory-region = <&bifbase_reserved>;
			status = "disabled";
		};

		bifeth: bifeth {
			compatible = "hobot,bifeth";
			status = "disabled";
		};

		mcu_irq: mcu_irq {
			compatible = "hobot,mcu_irq";
			status = "disabled";
		};
/*
		biflite_spi: biflite_spi {
			compatible = "hobot,bif_lite_spi";
		};

		biflite_sd: biflite_sd {
			compatible = "hobot,bif_lite_sd";
		};
*/
		temp: temp@0xA1009000 {
			compatible = "hobot,x2-temp";
			reg = <0 0xA1009000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 53 4>;
			#thermal-sensor-cells = <0>;
			status = "disabled";
		};

		ddr_monitor: ddr_monitor@0xA2D10000 {
			compatible = "hobot,x2-ddrmonitor";
			reg = <0 0xA2D10000 0 0x1000>;
			interrupt-parent = <&gic>;
			interrupts = <0 52 4>;
			status = "disabled";
		};

		onewire: onewire {
			compatible = "w1-gpio";
			onewire_gpio = <84>;
			status = "disabled";
		};

		power: power_management@0xA6000000 {
			compatible = "hobot,x2-power";
			reg = <0x0 0xA6000000 0x0 0x1000>;
			reboot-offset = <0x20>;
		};

		cnn_opp_table: cnn_opp_table {
			compatible = "operating-points-v2";
			opp-shared;
			opp00 {
				opp-hz = /bits/ 64 <800000000>;
				opp-microvolt = <1000000>;
				clock-latency-ns = <500000>;
			};
			opp01 {
				opp-hz = /bits/ 64 <540000000>;
				opp-microvolt = <900000>;
				clock-latency-ns = <500000>;
			};
			opp02 {
				opp-hz = /bits/ 64 <400000000>;
				opp-microvolt = <800000>;
				clock-latency-ns = <500000>;
			};
			opp03 {
				opp-hz = /bits/ 64 <320000000>;
				opp-microvolt = <800000>;
				clock-latency-ns = <500000>;
			};
			opp04 {
				opp-hz = /bits/ 64 <200000000>;
				opp-microvolt = <800000>;
				clock-latency-ns = <500000>;
			};
		};

		efuse: efuse@0xA1001000{
			compatible = "hobot,x2-efuse";
			reg = <0x0 0xA1001000 0x0 0x1000>,
				<0x0 0xA1000400 0x0 0x4>;
		};

		timesync: timesync {
			compatible = "timesync";
		};
	};
};
