#include "memory.dtsi"
/ {
	reserved-memory {

		iar_reserved: iar_reserved@0x02900000 {
			reg = <0x0 0x02900000 0x0 0x2000000>;
			no-map;
		};

		/* Reserved 1G memory for ion*/
		ion_reserved: ion_reserved@0x04900000{
			compatible = "ion-pool";
			reg = <0x0 0x04900000 0x0 0x40800000>;
		};
	};
};
