					39	0
					40	0
					41	0
					42	0
					43	0
					44	0
					45	0
				>;
			};
			pwm0:pwm0_func {
				pinctrl-x2,pins-muxs = <
					4	2
				>;
			};
			pwm1:pwm1_func {
				pinctrl-x2,pins-muxs = <
					5	2
				>;
			};
			pwm2:pwm2_func {
				pinctrl-x2,pins-muxs = <
					6	2
				>;
			};
			pwm3:pwm3_func {
				pinctrl-x2,pins-muxs = <
					7	2
				>;
			};
			pwm4:pwm4_func {
				pinctrl-x2,pins-muxs = <
					8	2
				>;
			};
			pwm5:pwm5_func {
				pinctrl-x2,pins-muxs = <
					72	2
				>;
			};
			pwm6:pwm6_func {
				pinctrl-x2,pins-muxs = <
					73	2
				>;
			};
			pwm7:pwm7_func {
				pinctrl-x2,pins-muxs = <
					74	2
				>;
			};
			pwm8:pwm8_func {
				pinctrl-x2,pins-muxs = <
					75	2
				>;
			};
		};
	};
};
