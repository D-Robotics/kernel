#include "memory.dtsi"
/ {
	reserved-memory {

		/*Reserved 32M memory for iar subsystem*/
		iar_reserved: iar_reserved@0x02900000 {
			reg = <0x0 0x02900000 0x0 0x2000000>;
			no-map;
		};

		/* Reserved 768MB memory for ion*/
		ion_reserved: ion_reserved@0x04900000 {
			compatible = "ion-pool";
			reg = <0x0 0x04900000 0x0 0x30800000>;
		};

	};
};
